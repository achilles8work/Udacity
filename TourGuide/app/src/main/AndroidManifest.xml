<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhishek.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Mall"
            android:label="@string/category_mall"/>

        <activity android:name=".Park"
            android:label="@string/category_park"/>

        <activity android:name=".Startup"
            android:label="@string/category_startup"/>
        
        <activity android:name=".Restaurant"
            android:label="@string/category_restaurant"></activity>
    </application>

</manifest>